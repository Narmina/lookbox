$("#modal-form").modal('hide');

<% if @picture.image_timestamp.present? %>
  $("#picture-<%=@picture.id%>").html("<%= escape_javascript (render(partial: 'pictures/picture', locals: {picture: @picture}).force_encoding("utf-8")) %>")
<% elsif @picture.previous_changes['title'].present? %>
  $("#image-block-<%= @picture.id %>").siblings('.picture-title').html("<%= @picture.title%>");
<% end %>

